@model Common.Models.ErrorLogModel

@{
    ViewData["Title"] = "Szczegóły błędu";
}

<h2>Szczegóły błędu</h2>

<div>
    <h4>Błąd: @Model.Message</h4>
    <hr />
    <dl class="dl-horizontal">
        <dt>
            @Html.DisplayNameFor(model => model.ObjectId)
        </dt>
        <dd>
            @Html.DisplayFor(model => model.ObjectId)
        </dd>
        <dt>
            @Html.DisplayNameFor(model => model.DateTime)
        </dt>
        <dd>
            @Html.DisplayFor(model => model.DateTime)
        </dd>
        <dt>
            @Html.DisplayNameFor(model => model.ClassName)
        </dt>
        <dd>
            @Html.DisplayFor(model => model.ClassName)
        </dd>
        <dt>
            @Html.DisplayNameFor(model => model.StackTrace)
        </dt>
        <dd>
            <span style="white-space: pre-line">@Model.StackTrace</span>
        </dd>
    </dl>
</div>
<div>
    <button onclick="location.href='@Url.Action("Errors","Logger")'"
            class="btn btn-default">
        <span class="glyphicon glyphicon-arrow-left"></span>
        <span>Powrót</span>
    </button>
</div>
